{
  "expo": {
    "name": "Mars Radio",
    "slug": "marsx",
    "scheme": "marsx",
    "version": "7.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/a8640a18-c37e-4c8b-8b18-48f78543849d"
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "ios": {
      "supportsTablet": true,
      "buildNumber": "7.0.0",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.mystseed.marsx"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mystseed.marsx",
      "googleServicesFile": "./google-services.json",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "mars-radio.app",
              "pathPrefix": "/video"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "plugins": [
      "expo-router",
      "react-native-edge-to-edge",
      "expo-font",
      "expo-secure-store",
      "expo-video"
    ],
    "runtimeVersion": "1.0",
    "extra": {
      "router": {
        "origin": false,
        "config": {
          "screens": {
            "(drawer)": {
              "screens": {
                "(tabs)": {
                  "screens": {
                    "video": {
                      "path": "video/:id"
                    }
                  }
                }
              }
            }
          }
        }
      },
      "eas": {
        "projectId": "a8640a18-c37e-4c8b-8b18-48f78543849d"
      }
    },
    "owner": "myst-seed"
  }
}