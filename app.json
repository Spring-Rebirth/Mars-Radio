{
  "expo": {
    "name": "MarsX",
    "slug": "marsx",
    "scheme": "marsx",
    "version": "2.0.0-beta",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "updates": {
      "fallbackToCacheTimeout": 0,
