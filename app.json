{
  "expo": {
    "name": "Mars Radio",
    "slug": "mars-radio",
    "scheme": "mars-radio",
    "version": "3.8.3",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/6b5d4a34-bfab-4bd4-bee9-8483c1128e5d"
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mystseed.marsradio",
      "buildNumber": "3.8.3",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mystseed.marsradio",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "plugins": [
      "expo-router",
      "react-native-edge-to-edge",
      "expo-font",
      "expo-secure-store",
      "expo-video"
    ],
    "runtimeVersion": "3.0",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "6b5d4a34-bfab-4bd4-bee9-8483c1128e5d"
      }
    },
    "owner": "myst-seed"
  }
}
